FROM 10.9.49.222:5000/centos

MAINTAINER wangfei "wangfei@chunyu.me"

RUN mkdir -p /usr/local/ip/
RUN mkdir -p /usr/local/ip/log

ADD ip_query/qqwry.dat /usr/local/ip/qqwry.dat
ADD iplocation /usr/local/ip/

ADD control.sh /usr/local/ip/
ADD config.test.ini /usr/local/ip/config.test.ini
ADD config.online.ini /usr/local/ip/config.online.ini

EXPOSE 80

PUSH 10.9.49.222:5000/iplocation:v2


